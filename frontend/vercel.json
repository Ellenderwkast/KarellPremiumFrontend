{
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "https://karellpremiumbackend.onrender.com/sitemap.xml" },
    { "source": "/robots.txt", "destination": "https://karellpremiumbackend.onrender.com/robots.txt" }
  ],
  "routes": [
    { "src": "/(.*\\..*)", "dest": "/$1" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
